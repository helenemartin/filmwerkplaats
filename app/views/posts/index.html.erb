<h1>Listing public posts</h1>

<% @posts.each do |post| %>
  <div class="row">
    <div class="span4">
      
      <%= simple_format post.description %>
      <br/>
      <%= link_to 'Show', post %>
      <%# render :partial => "admin_links", :locals => { :post => post } %>
    </div>
  </div>
<% end %>
